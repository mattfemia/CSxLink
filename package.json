{
  "name": "CSxLink",
  "version": "0.0.1",
  "description": "Codesmith resident portfolio and networking application",
  "main": "webpack.config.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production SERVER_PROXY=https://www.csxlink.com/ node server/server.js & NODE_ENV=production SERVER_PROXY=https://www.csxlink.com/ webpack serve",
    "build-dev": "NODE_ENV=development webpack",
    "build-prod": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development webpack-dev-server --open & nodemon ./dist/server/server.js",
    "docker": "NODE_ENV=production node ./server/server.js",
    "zip": "git archive -v -o csxnetwork.zip --format=zip HEAD",
    "test": "jest --verbose --forceExit"
  },
  "jest": {
    "globalSetup": "./jest.setup.js",
    "globalTeardown": "./jest.teardown.js",
    "testEnvironment": "jest-environment-jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattfemia/CSxLink.git"
  },
  "keywords": [
    "codesmith"
  ],
  "author": "Matt Femia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattfemia/CSxLink/issues"
  },
  "homepage": "https://github.com/mattfemia/CSxLink#readme",
